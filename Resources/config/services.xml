<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <services>
        <service id="bacon_rd_station.api" class="Bacon\Bundle\RDStationBundle\Service\Api">
            <argument>%bacon_rd_station.api.private_token%</argument>
            <argument>%bacon_rd_station.api.token%</argument>
            <argument>%bacon_rd_station.api.base_url%</argument>
            <argument>%bacon_rd_station.api.version%</argument>
        </service>
    </services>

</container>
